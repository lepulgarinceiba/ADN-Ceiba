{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "primeng/*": ["node_modules/primeng-lts/*"],
      "@core/*": [
        "src/app/core/*"
      ],
      "@core-service/*": [
        "src/app/core/services/*"
      ],
      "@producto/*": [
        "src/app/feature/producto/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@products-list/*": [
        "src/app/feature/products-list/*"
      ],
      "@shopping-cart/*": [
        "src/app/feature/shopping-cart/*"
      ],
      "@shopping-history/*": [
        "src/app/feature/shopping-history/*"
      ],
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
